{
  "name": "modclub_assets",
  "version": "0.1.0",
  "description": "Modclub",
  "type": "module",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "JavaScript",
    "Canister"
  ],
  "scripts": {
    "build": "node ../../scripts/build/gen_files_by_env.cjs && webpack",
    "start": "export DEV_ENV=${DEV_ENV:-local} &&  node ../../scripts/build/gen_files_by_env.cjs && webpack serve --mode development --config webpack.config.cjs",
    "add_tags": "git tag -a $(date \"+%Y-%m-%d-%H.%M\") -m \"Deployed by ${GITHUB_ACTOR:-$(whoami)}\" && git push --tags",
    "add_tags_prod": "git tag -a PROD-$(date \"+%Y-%m-%d-%H.%M\") -m \"Deployed to PROD by ${GITHUB_ACTOR:-$(whoami)}\" && git push --tags",
    "copy:types": "rsync -avr ../../.dfx/$(echo ${DFX_NETWORK:-'**'})/canisters/** --exclude='assets/' --exclude='idl/' --exclude='*.wasm' --delete ../../src/declarations",
    "deploy_prod_frontend": "export DEV_ENV=production && source ../../scripts/utils.sh && generate_declarations prod ic && node ../../scripts/build/gen_files_by_env.cjs && dfx generate modclub_assets --network ic && dfx deploy modclub_assets --network ic && npm run add_tags_prod",
    "prepare": "cd ../../ && husky install"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "assert": "^2.1.0",
