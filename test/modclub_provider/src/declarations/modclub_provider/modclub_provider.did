type ContentStatus = 
 variant {
   approved;
   rejected;
   reviewRequired;
 };
type ContentResult = 
 record {
   sourceId: text;
   status: ContentStatus;
 };
service : {
  deregister: () -> (text);
  greet: (text) -> (text);
  submitImage: (text, vec nat8, text, text) -> (text);
  submitText: (text, text, text) -> (text);
  subscribe: () -> ();
  test: () -> (text);
  voteResult: (ContentResult) -> () oneway;
}
